{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "posts-bridge/remote-field",
  "version": "1.0.0",
  "title": "Remote Field",
  "category": "widgets",
  "description": "Remote field custom block that replace marks by its remote value on the render.",
  "icon": "airplane",
  "supports": {
    "anchor": true,
    "html": false
